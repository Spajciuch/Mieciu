const Discord = require("discord.js");
const config = require(`../config.json`)
module.exports.run = async (client, message, args) => {
 message.channel.createWebhook("TestHook", "https://cdn.steemitimages.com/DQmdBKEXjvjSBW7JCLVRKqdYPGzCFQSRTmLY3yoBNUtEzb1/Test-Logo-Small-Black-transparent-1.png")
.then(webhook => webhook.edit("TestHook", "https://cdn.steemitimages.com/DQmdBKEXjvjSBW7JCLVRKqdYPGzCFQSRTmLY3yoBNUtEzb1/Test-Logo-Small-Black-transparent-1.png")
.then(wb => message.author.send(`Tw√≥j WebHook https://canary.discordapp.com/api/webhooks/${wb.id}/${wb.token}`)).catch(console.error))
}
module.exports.help = {
  name: "testhook"
}
